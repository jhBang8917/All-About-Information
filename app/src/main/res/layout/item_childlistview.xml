<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
        >
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" android:id="@+id/childlistview"
            android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="0dp"
                android:layout_height="wrap_content" android:layout_weight="4.5" android:layout_gravity="center"
                android:gravity="center_vertical|center_horizontal" android:paddingLeft="20dp">
        </LinearLayout>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="6" android:layout_marginRight="15dp" android:layout_gravity="center"
                android:layout_marginLeft="15dp">
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:text="내용1"
                    android:id="@+id/title1_child_textview" android:layout_column="3" android:layout_row="1"
                    android:ellipsize="marquee" android:singleLine="true"
                    android:layout_weight="1" android:textColor="#70000000" android:textSize="11dp"
                    android:textStyle="bold"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="content1"
                    android:id="@+id/content1_child_textview" android:layout_column="3" android:layout_row="2"
                    android:layout_weight="1" android:layout_marginLeft="10dp" android:textSize="16dp"
                    android:textColor="#ff000000"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:id="@+id/space1" android:layout_weight="1"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="내용2"
                    android:id="@+id/title2_child_textview" android:layout_column="3" android:layout_row="3"
                    android:layout_weight="1" android:textColor="#70000000" android:textSize="11dp"
                    android:textStyle="bold"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Medium Text"
                    android:id="@+id/content2_child_textview" android:layout_row="4" android:layout_column="3"
                    android:singleLine="false"
                    android:layout_weight="1" android:layout_marginLeft="10dp" android:textSize="16dp"
                    android:textColor="#ff000000"/>
            <TextView android:layout_width="match_parent" android:layout_height="0dp"
                      android:textAppearance="?android:attr/textAppearanceSmall" android:id="@+id/textView4"
                      android:layout_weight="1"/>
            <TextView android:layout_width="match_parent" android:layout_height="0dp"
                      android:textAppearance="?android:attr/textAppearanceSmall" android:text="내용2"
                      android:id="@+id/title3_child_textview" android:layout_column="3" android:layout_row="3"
                      android:ellipsize="marquee" android:singleLine="true"
                      android:layout_weight="1" android:textColor="#70000000" android:textSize="11dp"
                      android:textStyle="bold"/>
            <TextView android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceSmall" android:text="Medium Text"
                      android:id="@+id/content3_child_textview" android:layout_row="4" android:layout_column="3"
                      android:singleLine="false"
                      android:layout_weight="1" android:layout_marginLeft="10dp" android:textSize="16dp"
                      android:textColor="#ff000000"/>
            <TextView android:layout_width="match_parent" android:layout_height="0dp"
                      android:textAppearance="?android:attr/textAppearanceSmall" android:id="@+id/textView5"
                      android:layout_weight="1"/>
            <TextView android:layout_width="match_parent" android:layout_height="0dp"
                      android:textAppearance="?android:attr/textAppearanceSmall"
                      android:id="@+id/title4_child_textview" android:layout_column="3" android:layout_row="3"
                      android:ellipsize="marquee" android:singleLine="true"
                      android:layout_weight="1" android:textColor="#70000000" android:textSize="11dp"
                      android:textStyle="bold"/>
            <TextView android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceSmall" android:text="Medium Text"
                      android:id="@+id/content4_child_textview" android:layout_row="4" android:layout_column="3"
                      android:singleLine="false"
                      android:layout_weight="1" android:layout_marginLeft="10dp" android:textSize="16dp"
                      android:textColor="#ff000000"/>
            <TextView android:layout_width="match_parent" android:layout_height="0dp"
                      android:textAppearance="?android:attr/textAppearanceSmall" android:id="@+id/textView6"
                      android:layout_weight="1"/>
            <TextView android:layout_width="match_parent" android:layout_height="0dp"
                      android:textAppearance="?android:attr/textAppearanceSmall" android:text="내용2"
                      android:id="@+id/title5_child_textview" android:layout_column="3" android:layout_row="3"
                      android:ellipsize="marquee" android:singleLine="true"
                      android:layout_weight="1" android:textColor="#70000000" android:textSize="11dp"
                      android:textStyle="bold"/>
            <TextView android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceSmall" android:text="Medium Text"
                      android:id="@+id/content5_child_textview" android:layout_row="4" android:layout_column="3"
                      android:singleLine="false"
                      android:layout_weight="1" android:layout_marginLeft="10dp" android:textSize="16dp"
                      android:textColor="#ff000000"/>
            <TextView android:layout_width="match_parent" android:layout_height="0dp"
                      android:textAppearance="?android:attr/textAppearanceSmall" android:id="@+id/textView3"
                      android:layout_weight="1"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>